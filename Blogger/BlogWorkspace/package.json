{
  "name": "Blogger-workspace",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng run Blogger:server:dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "watch-app1": "ng serve --project my-first-app --port 15000",
    "start1": "concurrently --kill-others  \"sleep 5\" \"ng serve --project my-first-app --port 15000\" ",
    "my": "npm run watch-lib & start1",
    "watch-lib:my-library2": "wait-on dist/my-lib/public-api.d.ts && ng build my-library2 --watch",
    "watch-all0": "npm run clean:dist && concurrently --kill-others --restart-tries 3 \"npm run watch-lib:my-library1\" \"npm run serve:app\" \"npm run serve:app-2\"",
    "start2": "ng serve --project my-second-app --port 16000",
    "serve:app": "wait-on dist/my-lib/public-api.d.ts && ng serve --project my-first-app --port 15000",
    "serve:app-2": "wait-on dist/my-lib/public-api.d.ts && ng serve --project my-second-app --port 16000",
    "compodoc": "npx compodoc -p projects/my-first-app/tsconfig.app.json",
    "clean": "rimraf node_modules",
    "clean-all": "rimraf node_modules dist dll && npm cache clean",
    "clean:dist": "rimraf dist",
    "reinstall": "rimraf node_modules && rm -f package-lock.json && npm install",
    "watch-lib:services-api": "ng build services-api --watch",
    "watch-lib:shared-lib": "wait-on dist/services-api/public-api.d.ts && ng build shared-lib --watch",
    "serve:Blogger-app": "wait-on dist/services-api/public-api.d.ts && ng serve --project BloggerApp --port 8000",
    "serve:Blogger-app-net": "wait-on dist/services-api/public-api.d.ts && ng serve --project BloggerApp",
    "serve-all": "concurrently --kill-others --restart-tries 3 \"npm run watch-lib:services-api\" \"npm run serve:Blogger-app\"",
    "start-all": "npm run clean:dist && concurrently --kill-others --restart-tries 3 \"npm run watch-lib:services-api\" \"npm run serve:Blogger-app\"",
    "ng:build:dev": "echo Starting... && concurrently --kill-others --restart-tries 3 \"npm run watch-lib:services-api\" \"ng build --project BloggerApp\" ",
    "ng:build:stag": "ng build --project BloggerApp --configuration=stag --outputHashing=all ",
    "swag:build:auth": "nswag run .\\services-api.nswag.json",
    "start:mytest": "concurrently --kill-others --restart-tries 3 \"npm run watch-lib:web-service-api\" \"npm run watch-lib:shared-lib\" \"npm run watch-lib:auth-models\" \"npm run watch-lib:auth-mappers\" \"npm run watch-lib:auth-services\" \"npm run watch-lib:auth-components\" \"npm run serve:vd\" "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.5",
    "@angular/common": "~10.0.5",
    "@angular/compiler": "~10.0.5",
    "@angular/core": "~10.0.5",
    "@angular/forms": "~10.0.5",
    "@angular/platform-browser": "~10.0.5",
    "@angular/platform-browser-dynamic": "~10.0.5",
    "@angular/router": "~10.0.5",
    "concurrently": "^5.3.0",
    "nswag": "^13.6.2",
    "rimraf": "^3.0.2",
    "@coreui/angular": "~2.9.4",
    "@coreui/coreui": "^2.1.16",
    "@coreui/coreui-plugin-chartjs-custom-tooltips": "^1.3.1",
    "@coreui/icons": "0.3.0",
    "bootstrap": "^4.5.0",
    "chart.js": "^2.9.3",
    "core-js": "^3.6.5",
    "flag-icon-css": "^3.5.0",
    "font-awesome": "^4.7.0",
    "moment": "^2.27.0",
    "mutationobserver-shim": "^0.3.7",
    "ng2-charts": "^2.3.2",
    "ngx-bootstrap": "^5.6.1",
    "ngx-perfect-scrollbar": "^9.0.0",
    "rxjs": "^6.6.0",
    "simple-line-icons": "^2.5.2",
    "ts-helpers": "^1.1.2",
    "web-animations-js": "^2.3.2",
    "tslib": "^2.0.0",
    "wait-on": "^5.1.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.4",
    "@angular-devkit/build-ng-packagr": "~0.1000.4",
    "@angular/cli": "~10.0.4",
    "@angular/compiler-cli": "~10.0.5",
    "@types/node": "^12.11.1",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^10.0.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~3.9.5"
  },
  "engines": {
    "node": ">= 10.13",
    "npm": ">= 6"
  }
}
